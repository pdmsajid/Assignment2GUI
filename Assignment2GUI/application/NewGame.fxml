<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MainController">
   <children>
      <Label layoutX="115.0" layoutY="14.0" prefHeight="28.0" prefWidth="170.0" text="          Select  Game">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Button fx:id="selectRunningbtn" layoutX="119.0" layoutY="46.0" mnemonicParsing="false" onAction="#selectGame" prefHeight="104.0" prefWidth="163.0" text="Running" />
      <Button fx:id="selectSwimmingbtn" layoutX="119.0" layoutY="172.0" mnemonicParsing="false" onAction="#selectSwimmingGame" prefHeight="104.0" prefWidth="163.0" text="Swimming" />
      <Button fx:id="selectCyclingbtn" layoutX="119.0" layoutY="295.0" mnemonicParsing="false" onAction="#selectCyclingGame" prefHeight="104.0" prefWidth="163.0" text="Cycling" />
      <Button fx:id="backtoMain" layoutX="119.0" layoutY="422.0" mnemonicParsing="false" onAction="#backToMainMenu" prefHeight="56.0" prefWidth="163.0" text="Main Menu" />
   </children>
</AnchorPane>
